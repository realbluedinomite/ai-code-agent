{
  "name": "advanced-project",
  "version": "1.0.0",
  "description": "An advanced full-stack application with complex architecture, multiple microservices, and enterprise features",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/src/index.ts",
    "dev:client": "react-scripts start",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p server/tsconfig.json",
    "build:client": "react-scripts build",
    "start": "node server/dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint server/src client/src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint server/src client/src --ext .ts,.tsx,.js,.jsx --fix",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
