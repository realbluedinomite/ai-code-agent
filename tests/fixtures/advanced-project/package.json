    "rate-limiter-flexible": "^2.4.0",
    "socket.io": "^4.6.0",
    "redis": "^4.5.0",
    "bull": "^4.10.0",
    "bull-board": "^2.0.0",
    "winston": "^3.8.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "mongoose": "^7.0.0",
    "prisma": "^4.10.0",
    "@prisma/client": "^4.10.0",
    "knex": "^2.4.0",
    "pg": "^8.8.0",
    "multer": "^1.4.5",
    "sharp": "^0.31.0",
    "nodemailer": "^6.8.0",
    "aws-sdk": "^2.1310.0",
